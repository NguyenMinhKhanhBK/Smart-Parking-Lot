<Window
    x:Class="Smart_Parking_Lot.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dragablz="clr-namespace:Dragablz;assembly=Dragablz"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/charts"
    xmlns:local="clr-namespace:Smart_Parking_Lot"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:MyResource="clr-namespace:Smart_Parking_Lot.Resource"
    Width="1920"
    Height="1081"
    mc:Ignorable="d"
    Background="#FFEAEAEA"
    Icon="Image/icon.png"
    Title="Smart Parking Lot"
    WindowStartupLocation="Manual">

    <Window.Effect>
        <DropShadowEffect
            Color="#FFD6D6D6"
            BlurRadius="20"
            RenderingBias="Quality" />
    </Window.Effect>

    <Window.Resources>
        <Style BasedOn="{StaticResource MaterialDesignTabablzControlStyle}" TargetType="{x:Type dragablz:TabablzControl}">
            <Setter Property="ItemContainerStyle">
                <Setter.Value>
                    <Style BasedOn="{StaticResource MaterialDesignDragableTabItemStyle}" TargetType="dragablz:DragablzItem">
                        <Setter Property="Height" Value="70" />
                        <Setter Property="Width" Value="200" />
                        <Setter Property="FontSize" Value="20" />
                    </Style>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="itemHeaderStyle" TargetType="Grid">
            <Setter Property="Width" Value="180" />
            <Setter Property="Height" Value="35" />
            <Setter Property="Margin" Value="5 0 0 0" />
        </Style>
    </Window.Resources>

    <Grid>
        <dragablz:TabablzControl
            Margin="0,0,0,-5"
            BorderThickness="2,0,2,2"
            FixedHeaderCount="5"
            Padding="2">
            <!--MAN HINH CHINH-->
            <TabItem>
                <!--Declare header name-->
                <TabItem.Header>
                    <Grid VerticalAlignment="Center">
                        <materialDesign:PackIcon
                            Width="20"
                            Height="20"
                            Margin="-10,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Foreground="White"
                            Kind="HomeOutline" />
                        <TextBlock
                            Margin="20,0,0,0"
                            FontSize="20"
                            Foreground="White"
                            Text="Màn hình chính" />
                    </Grid>
                </TabItem.Header>
                <!--Main grid in MAN HINH CHINH-->
                <Grid>
                    <!--Divide the grid into 2 cols and 2 rows-->
                    <!--First col for the parking lot diagram-->
                    <!--The other col for the list of current customer, car counter and alarms-->
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*" />
                        <ColumnDefinition Width="1*" />
                        <ColumnDefinition Width="1*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1.5*" />
                        <RowDefinition Height="2*" />
                    </Grid.RowDefinitions>
                    <!--PARKING LOT DIAGRAM-->
                    <Border
                        Grid.Row="0"
                        Grid.RowSpan="2"
                        Grid.Column="0"
                        Margin="20,20,10,30"
                        Background="White"
                        BorderBrush="White"
                        BorderThickness="0"
                        CornerRadius="8">
                        <Border.Effect>
                            <DropShadowEffect
                                Color="#FFC1C1C1"
                                BlurRadius="20"
                                RenderingBias="Quality"
                                ShadowDepth="5" />
                        </Border.Effect>
                        <Grid Grid.Row="0" Grid.Column="0">
                            <!--Name: So do bai xe-->
                            <TextBlock
                                Margin="40,10"
                                FontSize="40"
                                Foreground="#FF5D00FF"
                                Text="Sơ đồ bãi xe">
                                <!--<TextBlock.Effect>
                                    <DropShadowEffect Color="#FFC5C5C5" RenderingBias="Quality" BlurRadius="30" ShadowDepth="2"/>
                                </TextBlock.Effect>-->
                            </TextBlock>
                            <!--Divide the grid into 12 parts for each station-->
                            <Grid Margin="0,60,0,10">
                                <Grid.Effect>
                                    <DropShadowEffect
                                        Color="#FFB9B9B9"
                                        BlurRadius="20"
                                        RenderingBias="Quality" />
                                </Grid.Effect>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1*" />
                                    <RowDefinition Height="1*" />
                                    <RowDefinition Height="1*" />
                                    <RowDefinition Height="1*" />
                                    <RowDefinition Height="1*" />
                                    <RowDefinition Height="1*" />
                                </Grid.RowDefinitions>
                                <!--Row 0 - Col 0 slot-->
                                <Grid Grid.Row="0" Grid.Column="0">
                                    <MyResource:gridBooked />
                                </Grid>
                                <!--Row 0 - Col 1 slot-->
                                <Grid
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridAvailable />
                                </Grid>
                                <!--Row 1 - Col 0 slot-->
                                <Grid
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridOccupied />
                                </Grid>
                                <!--Row 1 - Col 1 slot-->
                                <Grid
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridAvailable />
                                </Grid>
                                <!--Row 2 - Col 0 slot-->
                                <Grid
                                    Grid.Row="2"
                                    Grid.Column="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridBooked />
                                </Grid>
                                <!--Row 2 - Col 1 slot-->
                                <Grid
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridBooked />
                                </Grid>
                                <!--Row 3 - Col 0 slot-->
                                <Grid
                                    Grid.Row="3"
                                    Grid.Column="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridBooked />
                                </Grid>
                                <!--Row 3 - Col 1 slot-->
                                <Grid
                                    Grid.Row="3"
                                    Grid.Column="1"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridBooked />
                                </Grid>
                                <!--Row 4 - Col 0 slot-->
                                <Grid
                                    Grid.Row="4"
                                    Grid.Column="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridBooked />
                                </Grid>
                                <!--Row 4 - Col 1 slot-->
                                <Grid
                                    Grid.Row="4"
                                    Grid.Column="1"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridBooked />
                                </Grid>
                                <!--Row 5 - Col 0 slot-->
                                <Grid
                                    Grid.Row="5"
                                    Grid.Column="0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridBooked />
                                </Grid>
                                <!--Row 5 - Col 1 slot-->
                                <Grid
                                    Grid.Row="5"
                                    Grid.Column="1"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                    <MyResource:gridBooked />
                                </Grid>
                            </Grid>
                        </Grid>
                    </Border>
                    <!--CAR COUNTER-->
                    <Grid Grid.Row="0" Grid.Column="1">
                        <Border
                            Margin="0,20"
                            Background="White"
                            BorderBrush="White"
                            BorderThickness="0"
                            CornerRadius="8">
                            <Border.Effect>
                                <DropShadowEffect
                                    Color="#FFC1C1C1"
                                    BlurRadius="20"
                                    RenderingBias="Quality"
                                    ShadowDepth="5" />
                            </Border.Effect>
                            <StackPanel>
                                <TextBlock
                                    Margin="40 10 0 0"
                                    FontSize="40"
                                    Foreground="#FF5D00FF"
                                    Text="Thống kê hiện tại">
                                    <!--<TextBlock.Effect>
                                        <DropShadowEffect Color="#FFC5C5C5" RenderingBias="Quality" BlurRadius="30" ShadowDepth="2"/>
                                    </TextBlock.Effect>-->
                                </TextBlock>
                                <Grid>
                                    <dxc:ChartControl Margin="0 0 0 0">
                                        <dxc:ChartControl.Legends>
                                            <dxc:Legend
                                                FontSize="10"
                                                Foreground="#FF050505"
                                                HorizontalPosition="Left"
                                                IndentFromDiagram="-10"
                                                ItemVisibilityMode="Default"
                                                MarkerMode="Marker"
                                                Orientation="Vertical"
                                                ReverseItems="False"
                                                VerticalPosition="Top"
                                                Visible="True">
                                                <dxc:Legend.Title>
                                                    <dxc:LegendTitle
                                                        HorizontalAlignment="Center"
                                                        Content="Tổng số chỗ: 12"
                                                        Visible="{x:Null}" />
                                                </dxc:Legend.Title>
                                            </dxc:Legend>
                                        </dxc:ChartControl.Legends>
                                        <dxc:SimpleDiagram2D
                                            Margin="20"
                                            HorizontalAlignment="Right"
                                            VerticalAlignment="Bottom">
                                            <dxc:NestedDonutSeries2D
                                                DisplayName="Thống kê hiện tại"
                                                LabelsVisibility="False"
                                                LegendTextPattern="{}{A}:   {V}"
                                                ShowInLegend="True"
                                                ToolTipPointPattern="{}{V}"
                                                Visible="True"
                                                Weight="2">
                                                <dxc:SeriesPoint
                                                    Value="5"
                                                    Argument="Số chỗ trống"
                                                    Brush="#FF00C52A" />
                                                <dxc:SeriesPoint
                                                    Value="3"
                                                    Argument="Số chỗ được đặt"
                                                    Brush="#FFFFD100" />
                                                <dxc:SeriesPoint
                                                    Value="4"
                                                    Argument="Số chỗ có xe"
                                                    Brush="Red" />
                                            </dxc:NestedDonutSeries2D>
                                        </dxc:SimpleDiagram2D>
                                    </dxc:ChartControl>
                                </Grid>
                            </StackPanel>
                        </Border>
                    </Grid>
                    <!--ALARM-->
                    <Grid Grid.Row="0" Grid.Column="2">
                        <Border
                            Margin="10,20,10,20"
                            Background="White"
                            BorderBrush="White"
                            BorderThickness="0"
                            CornerRadius="8">
                            <Border.Effect>
                                <DropShadowEffect
                                    Color="#FFC1C1C1"
                                    BlurRadius="20"
                                    RenderingBias="Quality"
                                    ShadowDepth="5" />
                            </Border.Effect>
                            <Grid>
                                <!--Title: Trang thai haot dong-->
                                <TextBlock
                                    Margin="40,10"
                                    FontSize="40"
                                    Foreground="#FF7400FF"
                                    Text="Trạng thái hoạt động" />
                                <UniformGrid
                                    Margin="10,80,10,20"
                                    Columns="3"
                                    Rows="1">
                                    <!--Motor status-->
                                    <Grid>
                                        <Image
                                            Height="70"
                                            Margin="0,60,0,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Top"
                                            Source="Image/motorGreen.png" />
                                        <TextBlock
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Bottom"
                                            FontSize="60"
                                            Foreground="#FF01AB01"
                                            Text="Tốt" />
                                    </Grid>
                                    <!--Sensor status-->
                                    <Grid>
                                        <Image
                                            Height="80"
                                            Margin="0,60,0,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Top"
                                            Source="Image/carSensorGreen.png" />
                                        <TextBlock
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Bottom"
                                            FontSize="60"
                                            Foreground="#FF01AB01"
                                            Text="Tốt" />
                                    </Grid>
                                    <!--Detector status-->
                                    <Grid>
                                        <Image
                                            Height="70"
                                            Margin="0,60,0,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Top"
                                            Source="Image/detectorGreen.png" />
                                        <TextBlock
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Bottom"
                                            FontSize="35"
                                            Foreground="#FF01AB01"
                                            Text="Không có vật cản"
                                            TextWrapping="Wrap" />
                                    </Grid>
                                </UniformGrid>
                            </Grid>
                        </Border>
                    </Grid>
                    <!--CURRENT CUSTOMERS LIST-->
                    <Grid
                        Grid.Row="1"
                        Grid.Column="1"
                        Grid.ColumnSpan="2">
                        <Border
                            Margin="0,-10,10,30"
                            Background="White"
                            BorderBrush="White"
                            BorderThickness="0"
                            CornerRadius="8">
                            <Border.Effect>
                                <DropShadowEffect
                                    Color="#FFC1C1C1"
                                    BlurRadius="20"
                                    RenderingBias="Quality"
                                    ShadowDepth="5" />
                            </Border.Effect>
                            <StackPanel>
                                <StackPanel Margin="40 10 0 0" Orientation="Horizontal">
                                    <TextBlock
                                        FontSize="40"
                                        Foreground="#FF5D00FF"
                                        Text="Danh sách khách hàng" />
                                    <materialDesign:ColorZone
                                        Width="770"
                                        Margin="40 10 0 0"
                                        FontSize="20"
                                        Mode="PrimaryMid"
                                        Padding="5">
                                        <DockPanel>
                                            <StackPanel materialDesign:RippleAssist.IsCentered="True" Orientation="Horizontal">
                                                <materialDesign:ColorZone
                                                    Margin="5 0 0 0"
                                                    Panel.ZIndex="1"
                                                    CornerRadius="2"
                                                    materialDesign:ShadowAssist.ShadowDepth="Depth1"
                                                    Mode="Standard"
                                                    Padding="8 4 8 4">
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto" />
                                                            <ColumnDefinition Width="*" />
                                                            <ColumnDefinition Width="Auto" />
                                                        </Grid.ColumnDefinitions>
                                                        <Button Style="{DynamicResource MaterialDesignToolButton}">
                                                            <materialDesign:PackIcon Kind="Magnify" Opacity=".56" />
                                                        </Button>
                                                        <TextBox
                                                            Grid.Column="1"
                                                            MinWidth="360"
                                                            Margin="8 0 0 0"
                                                            VerticalAlignment="Center"
                                                            BorderThickness="0"
                                                            materialDesign:HintAssist.Hint="Nhập nội dung tìm kiếm"
                                                            materialDesign:TextFieldAssist.DecorationVisibility="Hidden" />
                                                    </Grid>
                                                </materialDesign:ColorZone>
                                                <ComboBox
                                                    Width="Auto"
                                                    MinWidth="150"
                                                    Margin="40,0,0,0"
                                                    VerticalAlignment="Center"
                                                    materialDesign:HintAssist.Hint="Tìm theo">
                                                    <ComboBoxItem>ID</ComboBoxItem>
                                                    <ComboBoxItem>Tên khách hàng</ComboBoxItem>
                                                    <ComboBoxItem>Biển số xe</ComboBoxItem>
                                                    <ComboBoxItem>Số điện thoại</ComboBoxItem>
                                                    <ComboBoxItem>Email</ComboBoxItem>
                                                    <ComboBoxItem>Trạng thái</ComboBoxItem>
                                                </ComboBox>
                                                <Button
                                                    Width="60"
                                                    Height="40"
                                                    Margin="30 0 0 0"
                                                    Panel.ZIndex="0"
                                                    Style="{DynamicResource MaterialDesignToolForegroundButton}">
                                                    <materialDesign:PackIcon
                                                        Width="40"
                                                        Height="35"
                                                        Foreground="White"
                                                        Kind="Send" />
                                                </Button>
                                            </StackPanel>
                                        </DockPanel>
                                    </materialDesign:ColorZone>
                                </StackPanel>
                                <Grid
                                    Margin="5"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Stretch">
                                    <ListView FontSize="18" ItemsSource="{Binding Items1}">
                                        <ListView.View>
                                            <GridView>
                                                <GridViewColumn DisplayMemberBinding="{Binding ID}" Header="ID" />
                                                <GridViewColumn DisplayMemberBinding="{Binding Name}" Header="Tên khách hàng" />
                                                <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Biển số xe" />
                                                <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Số điện thoại" />
                                                <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Email" />
                                                <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Trạng thái" />
                                            </GridView>
                                        </ListView.View>
                                    </ListView>
                                </Grid>
                                <!--NEXT ITEM WILL BE HERE-->
                            </StackPanel>
                        </Border>
                    </Grid>
                </Grid>
            </TabItem>
            <!-- ************************************************************************************************-->
            <!--THIET LAP-->
            <TabItem>
                <TabItem.Header>
                    <Grid VerticalAlignment="Center">
                        <materialDesign:PackIcon
                            Width="20"
                            Height="20"
                            Margin="-10,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Foreground="White"
                            Kind="SettingsOutline" />
                        <TextBlock
                            Margin="20,0,0,0"
                            FontSize="20"
                            Foreground="White"
                            Text="Thiết lập" />
                    </Grid>
                </TabItem.Header>
                <TabControl
                    Margin="0 5 0 5"
                    Background="White"
                    TabStripPlacement="Left">
                    <TabControl.BorderBrush>
                        <ImageBrush />
                    </TabControl.BorderBrush>
                    <TabControl.Effect>
                        <DropShadowEffect
                            Color="#FFE6E6E6"
                            BlurRadius="20"
                            RenderingBias="Quality"
                            ShadowDepth="2" />
                    </TabControl.Effect>
                    <!--Gia ve ngay thuong-->
                    <TabItem>
                        <TabItem.Header>
                            <Grid Style="{StaticResource ResourceKey=itemHeaderStyle}">
                                <TextBlock
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontSize="14"
                                    Text="Giá vé ngày thường" />
                            </Grid>
                        </TabItem.Header>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="200" />
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Column="1" Background="{x:Null}">
                                <Button
                                    Width="150"
                                    Margin="10"
                                    FontSize="18"
                                    materialDesign:RippleAssist.Feedback="#DD000000"
                                    Style="{StaticResource MaterialDesignRaisedAccentButton}"
                                    ToolTip="Resource name: MaterialDesignRaisedAccentButton">
                                    Thêm giá vé
                                </Button>
                                <Button
                                    Width="150"
                                    Margin="10"
                                    FontSize="18"
                                    materialDesign:RippleAssist.Feedback="#DD000000"
                                    Style="{StaticResource MaterialDesignRaisedAccentButton}"
                                    ToolTip="Resource name: MaterialDesignRaisedAccentButton">
                                    Chỉnh sửa
                                </Button>
                                <Button
                                    Width="150"
                                    Margin="10"
                                    FontSize="18"
                                    materialDesign:RippleAssist.Feedback="#DD000000"
                                    Style="{StaticResource MaterialDesignRaisedAccentButton}"
                                    ToolTip="Resource name: MaterialDesignRaisedAccentButton">
                                    Xoá
                                </Button>
                            </StackPanel>
                            <!--Bang gia ve-->
                            <Grid Grid.Column="0">
                                <ScrollViewer>
                                    <StackPanel>
                                        <!--Thu 2-->
                                        <StackPanel Margin="10 20 0 20">
                                            <TextBlock
                                                Margin="10 0 0 -5"
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="Blue"
                                                Text="Thứ 2" />
                                            <ListView ItemsSource="{Binding Items1}">
                                                <ListView.View>
                                                    <GridView>
                                                        <GridViewColumn DisplayMemberBinding="{Binding Code}" Header="STT" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Name}" Header="Thời gian bắt đầu" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Thời gian kết thúc" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Giá vé (VNĐ)" />
                                                    </GridView>
                                                </ListView.View>
                                            </ListView>
                                            <Separator />
                                        </StackPanel>
                                        <!--Thu 3-->
                                        <StackPanel Margin="10 0 0 20">
                                            <TextBlock
                                                Margin="10 0 0 -5"
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="Blue"
                                                Text="Thứ 3" />
                                            <ListView ItemsSource="{Binding Items1}">
                                                <ListView.View>
                                                    <GridView>
                                                        <GridViewColumn DisplayMemberBinding="{Binding Code}" Header="STT" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Name}" Header="Thời gian bắt đầu" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Thời gian kết thúc" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Giá vé (VNĐ)" />
                                                    </GridView>
                                                </ListView.View>
                                            </ListView>
                                            <Separator />
                                        </StackPanel>
                                        <!--Thu 4-->
                                        <StackPanel Margin="10 0 0 20">
                                            <TextBlock
                                                Margin="10 0 0 -5"
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="Blue"
                                                Text="Thứ 4" />
                                            <ListView ItemsSource="{Binding Items1}">
                                                <ListView.View>
                                                    <GridView>
                                                        <GridViewColumn DisplayMemberBinding="{Binding Code}" Header="STT" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Name}" Header="Thời gian bắt đầu" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Thời gian kết thúc" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Giá vé (VNĐ)" />
                                                    </GridView>
                                                </ListView.View>
                                            </ListView>
                                            <Separator />
                                        </StackPanel>
                                        <!--Thu 5-->
                                        <StackPanel Margin="10 0 0 20">
                                            <TextBlock
                                                Margin="10 0 0 -5"
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="Blue"
                                                Text="Thứ 5" />
                                            <ListView ItemsSource="{Binding Items1}">
                                                <ListView.View>
                                                    <GridView>
                                                        <GridViewColumn DisplayMemberBinding="{Binding Code}" Header="STT" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Name}" Header="Thời gian bắt đầu" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Thời gian kết thúc" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Giá vé (VNĐ)" />
                                                    </GridView>
                                                </ListView.View>
                                            </ListView>
                                            <Separator />
                                        </StackPanel>
                                        <!--Thu 6-->
                                        <StackPanel Margin="10 0 0 20">
                                            <TextBlock
                                                Margin="10 0 0 -5"
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="Blue"
                                                Text="Thứ 6" />
                                            <ListView ItemsSource="{Binding Items1}">
                                                <ListView.View>
                                                    <GridView>
                                                        <GridViewColumn DisplayMemberBinding="{Binding Code}" Header="STT" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Name}" Header="Thời gian bắt đầu" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Thời gian kết thúc" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Giá vé (VNĐ)" />
                                                    </GridView>
                                                </ListView.View>
                                            </ListView>
                                            <Separator />
                                        </StackPanel>
                                        <!--Thu 7-->
                                        <StackPanel Margin="10 0 0 20">
                                            <TextBlock
                                                Margin="10 0 0 -5"
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="Blue"
                                                Text="Thứ 7" />
                                            <ListView ItemsSource="{Binding Items1}">
                                                <ListView.View>
                                                    <GridView>
                                                        <GridViewColumn DisplayMemberBinding="{Binding Code}" Header="STT" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Name}" Header="Thời gian bắt đầu" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Thời gian kết thúc" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Giá vé (VNĐ)" />
                                                    </GridView>
                                                </ListView.View>
                                            </ListView>
                                            <Separator />
                                        </StackPanel>
                                        <!--Chu nhat-->
                                        <StackPanel Margin="10 0 0 20">
                                            <TextBlock
                                                Margin="10 0 0 -5"
                                                VerticalAlignment="Center"
                                                FontSize="14"
                                                Foreground="Blue"
                                                Text="Chủ nhật" />
                                            <ListView ItemsSource="{Binding Items1}">
                                                <ListView.View>
                                                    <GridView>
                                                        <GridViewColumn DisplayMemberBinding="{Binding Code}" Header="STT" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Name}" Header="Thời gian bắt đầu" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Thời gian kết thúc" />
                                                        <GridViewColumn DisplayMemberBinding="{Binding Description}" Header="Giá vé (VNĐ)" />
                                                    </GridView>
                                                </ListView.View>
                                            </ListView>
                                            <Separator />
                                        </StackPanel>
                                    </StackPanel>
                                </ScrollViewer>
                            </Grid>
                        </Grid>
                    </TabItem>
